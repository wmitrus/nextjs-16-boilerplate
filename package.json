{
  "name": "temp-nextjs-scaffold",
  "version": "1.9.1",
  "private": true,
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config vitest.unit.config.ts --coverage",
    "test:watch": "vitest --config vitest.unit.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts --coverage",
    "test:all": "vitest run --config vitest.config.ts --coverage",
    "test:storybook": "vitest run --project storybook --coverage",
    "test:storybook:watch": "vitest --project storybook",
    "test:storybook:ui": "vitest --project storybook --ui",
    "e2e": "E2E_ENABLED=true NEXT_PUBLIC_E2E_ENABLED=true playwright test",
    "e2e:smoke": "E2E_ENABLED=true NEXT_PUBLIC_E2E_ENABLED=true playwright test e2e/home.spec.ts e2e/security.spec.ts e2e/auth.spec.ts --project=chromium",
    "e2e:auth": "node scripts/check-e2e-auth-env.mjs && E2E_ENABLED=true NEXT_PUBLIC_E2E_ENABLED=true playwright test e2e/users.spec.ts e2e/error-boundary.spec.ts --project=chromium",
    "e2e:full": "node scripts/check-e2e-auth-env.mjs && E2E_ENABLED=true NEXT_PUBLIC_E2E_ENABLED=true playwright test --project=chromium",
    "e2e:ui": "E2E_ENABLED=true NEXT_PUBLIC_E2E_ENABLED=true playwright test --ui",
    "e2e:ci": "E2E_ENABLED=true NEXT_PUBLIC_E2E_ENABLED=true pnpm build && playwright test --reporter=dot",
    "prepare": "husky",
    "commit": "cz",
    "skott:check:only": "skott --cwd=src --showCircularDependencies --displayMode=file-tree",
    "depcheck": "depcheck",
    "madge": "madge --circular --extensions ts,tsx src",
    "generate:secret": "node scripts/generate-secret.mjs",
    "env:init": "node scripts/setup-env.mjs",
    "env:check": "node scripts/check-env-consistency.mjs",
    "chromatic": "chromatic",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "semantic-release"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "tsc-files --noEmit"
    ],
    "*.{json,css,scss,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@clerk/nextjs": "^6.39.0",
    "@sentry/nextjs": "^10.40.0",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.3",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.1.1",
    "next": "16.1.6",
    "pino": "^10.3.1",
    "pino-logflare": "^0.5.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@clerk/testing": "^1.14.3",
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/eslintrc": "^3.3.4",
    "@playwright/test": "^1.58.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.6",
    "@semantic-release/npm": "^13.1.4",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@storybook/addon-a11y": "^10.2.13",
    "@storybook/addon-docs": "^10.2.13",
    "@storybook/addon-onboarding": "^10.2.13",
    "@storybook/addon-vitest": "^10.2.13",
    "@storybook/nextjs-vite": "^10.2.13",
    "@tailwindcss/postcss": "^4.2.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.11.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "babel-plugin-react-compiler": "^1.0.0",
    "chromatic": "^15.2.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.3",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsonc": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-storybook": "^10.2.13",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "jsonc-eslint-parser": "^2.4.2",
    "lint-staged": "^16.2.7",
    "madge": "^8.0.0",
    "msw": "^2.12.10",
    "pino-pretty": "^13.1.3",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "semantic-release": "^25.0.3",
    "skott": "^0.35.7",
    "storybook": "^10.2.13",
    "tailwindcss": "^4.2.1",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "typescript-eslint": "^8.56.1",
    "undici": "7.22.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18",
    "whatwg-fetch": "^3.6.20"
  },
  "pnpm": {
    "overrides": {
      "@isaacs/brace-expansion": "^5.0.1",
      "@typescript-eslint/utils": "^8.56.1",
      "minimatch": ">=10.2.3",
      "lodash": ">=4.17.23",
      "rollup": "^4.59.0",
      "serialize-javascript": ">=7.0.3",
      "undici": ">=6.23.0",
      "tmp": ">=0.2.4"
    },
    "onlyBuiltDependencies": [
      "msw",
      "sharp",
      "unrs-resolver"
    ],
    "patchedDependencies": {
      "depcheck@1.4.7": "patches/depcheck@1.4.7.patch",
      "@storybook/builder-vite@10.2.13": "patches/@storybook__builder-vite@10.2.13.patch"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
